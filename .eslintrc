{
	"root": true,
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"plugin:react/recommended"
	],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"react/prop-types": 0,
		"no-undef":"off"
	}
}